@model IEnumerable<User2>
@{
    ViewData["Title"] = "Data";
}
<div class="row">
    <div class="col-12">
        <a href="/Home/Index" class="btn btn-success">Register</a>
    </div>
</div>
<div class="row my-5">
    <div class="col-8 m-auto py-3">
        <table class="table" id="datatable">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">LastName</th>
                    <th scope="col">FirstName</th>
                    <th scope="col">Address</th>
                    <th scope="col">City</th>
                    <th scope="col">Edit</th>
                    <th scope="col">Delete</th>


                </tr>
            </thead>
            <tbody id="tbody">
                @foreach (var item in Model)
                {
                    <tr id="@item.PersonID">
                        <th scope="row">@item.PersonID</th>
                        <td>@item.LastName</td>
                        <td>@item.FirstName</td>
                        <td>@item.Address</td>
                        <td>@item.City</td>
                        <td><a href="/Home/EditProcess/@item.PersonID" class="btn btn-primary"><i class="fa-solid fa-pen-to-square"></i></a></td>
                        <td><a href="/Home/DeleteProcess/@item.PersonID" class="btn btn-danger"><i class="fa-solid fa-pen-to-square"></i></a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


